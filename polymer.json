{
  "builds": [
    {
      "browserCapabilities": ["es2015"],
      "preset": "es6-unbundled",
      "addServiceWorker": true,
      "addPushManifest": true,
      "stripComments": "true",
      "sourcemaps": "true"
    }
  ],
  "entrypoint": "index.html",
  "extraDependencies": [
    "bower_components/webcomponentsjs/*.js",
    "bower_components/app-storage/app-indexeddb-mirror/*.js",
    "server/app.yaml",
    "manifest.json"
  ],
  "fragments": [
    "src/view-dashboard.html",
    "src/view-setup.html",
    "src/view-plan.html",
    "src/view-track.html",
    "src/view-settings.html",
    "src/view-login.html",
    "src/view-404.html",
    "src/view-help.html",
    "src/view-term.html",
    "src/view-error.html",
    "src/view-offline.html",
    "src/lazy-resources.html"
  ],
  "lint": {
    "ignoreWarnings": ["undefined-elements", "set-unknown-attribute"],
    "rules": ["polymer-2"]
  },
  "shell": "src/view-app.html",
  "sources": ["src/**/*", "data/**/*", "images/**/*", "lib/**", "bower.json"]
}
